{
  "parser": "@typescript-eslint/parser",
  "extends": ["@wunderwerk/eslint-config/typescript"],
  "rules": {
    "jsdoc/tag-lines": [
      "warn",
      "always",
      {
        "count": 0,
        "startLines": 1
      }
    ],
    "@typescript-eslint/consistent-type-assertions": "off",
    "@typescript-eslint/no-non-null-assertion": "off"
  }
}
