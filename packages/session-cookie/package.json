{
  "name": "session-cookie",
  "private": true,
  "type": "module",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "ava": {
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader",
      "ts-node/esm"
    ]
  },
  "scripts": {
    "test": "NODE_NO_WARNINGS=1 ava",
    "typecheck": "tsc --project ./tsconfig.json"
  },
  "dependencies": {
    "@edge-runtime/cookies": "^3.2.2",
    "@hapi/iron": "^7.0.1"
  },
  "peerDependencies": {
    "next": "^13.4.12"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
