{
  "name": "next-session-project",
  "version": "0.0.0",
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@local/config-prettier": "workspace:0.0.1-beta.0",
    "syncpack": "^11.2.1",
    "turbo": "^1.10.13"
  },
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-all": "turbo run build lint test typecheck",
    "format:check": "prettier --check 'packages/*/**/*.{ts,tsx}'",
    "format:write": "prettier --write 'packages/*/**/*.{ts,tsx}' && syncpack format",
    "lint": "turbo run lint",
    "publish-packages": "turbo run build lint test typecheck && changeset publish",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "version-packages": "turbo run build lint test typecheck && changeset version && pnpm install --lockfile-only"
  }
}
